<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.worldweatheronline">

  <uses-permission android:name="android.permission.INTERNET"/>
  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:name=".App"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".ui.apilist.ApiListActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
      android:label="Weather Api"
      android:name=".ui.weather.WeatherApiActivity"/>
    <activity
      android:label="Timezone Api"
      android:name=".ui.timezone.TimezoneApiActivity"/>
    <activity
      android:label="Past Weather Api"
      android:name=".ui.weather.past.PastWeatherApiActivity"/>
    <activity
      android:label="Marine Api"
      android:name=".ui.marine.MarineApiActivity"/>

  </application>

</manifest>